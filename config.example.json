{
  "presets": {
    "quick": {
      "description": "Fast responses from efficient models",
      "models": ["gemini-2.0-flash", "grok-2"],
      "timeout_seconds": 60
    },
    "frontier": {
      "description": "Best-in-class models for complex reasoning",
      "models": ["gpt-4o", "gemini-2.0-pro", "grok-2"],
      "timeout_seconds": 180
    },
    "comprehensive": {
      "description": "All available API models",
      "models": ["gpt-4o", "gemini-2.0-pro", "gemini-2.0-flash", "grok-2"],
      "timeout_seconds": 300
    },
    "deep-research": {
      "description": "Deep research mode - async, browser-based",
      "models": ["chatgpt-deep-research"],
      "async": true,
      "requires_browser": true
    }
  },
  "models": {
    "gpt-4o": {
      "provider": "openai",
      "model_id": "gpt-4o",
      "type": "api",
      "max_tokens": 16000
    },
    "gpt-4o-mini": {
      "provider": "openai",
      "model_id": "gpt-4o-mini",
      "type": "api",
      "max_tokens": 16000
    },
    "gemini-2.0-pro": {
      "provider": "google",
      "model_id": "gemini-2.0-pro-exp",
      "type": "api",
      "max_tokens": 32000
    },
    "gemini-2.0-flash": {
      "provider": "google",
      "model_id": "gemini-2.0-flash",
      "type": "api",
      "max_tokens": 16000
    },
    "grok-2": {
      "provider": "xai",
      "model_id": "grok-2-latest",
      "type": "api",
      "max_tokens": 16000
    },
    "gpt-5-pro": {
      "provider": "openai",
      "type": "browser",
      "url": "https://chatgpt.com/?model=gpt-5-pro",
      "requires_browser": true
    },
    "chatgpt-deep-research": {
      "provider": "openai",
      "type": "browser",
      "url": "https://chatgpt.com/",
      "mode": "deep-research",
      "async": true,
      "typical_duration_minutes": 30,
      "requires_browser": true
    }
  },
  "defaults": {
    "preset": "frontier",
    "synthesis_depth": "executive",
    "max_tokens": 4000
  },
  "synthesis_depths": {
    "brief": "2-3 sentence summary highlighting key consensus and disagreements",
    "executive": "1-2 paragraph summary with bullet points for key insights",
    "full": "Comprehensive analysis preserving all unique insights, organised by topic"
  }
}
